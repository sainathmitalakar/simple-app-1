pipeline {
    agent any
    tools {
  maven 'maven3'
}

    stages {
        stage('checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'mygithub', url: 'https://github.com/sainathmitalakar/simple-app-1.git']]])
            }
        }
    }
    stages{
        stage('upload a war file')
        {
            nexusArtifactUploader artifacts: [[artifactId: 'simple-app', 
            classifier: '', 
            file: 'target/simple-app-3.0.0.war',
            type: 'war']], 
            credentialsId: '08e84467-bf0e-4029-908a-c0513175594d',
            groupId: 'in.javahome',
            nexusUrl: '15.207.20.56',
            nexusVersion: 'nexus2', 
            protocol: 'http',
            repository: 'http://15.207.20.56:8081/repository/simple-app-release/', 
            version: '3.0.0'
        }
    }
}
